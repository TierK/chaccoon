{
  "$schema": "node_modules/ng-openapi-gen/ng-openapi-gen-schema.json",
  "input": "./src/api/openapi.json",
  "output": "./src/app/api",
  "language": "typescript",
  "servicePrefix": "Api",
  "indexFile": true,
  "modelIndexFile": true,
  "useUnionTypes": true,
  "useFormControls": false,
  "useClassNames": true
}